{
	"compilerOptions": {
		"baseUrl": "./",
		"rootDir": "./",
		"module": "commonjs",
		"noImplicitReturns": false,
		"noUnusedLocals": false,
		"outDir": "dist",
		"sourceMap": false,
		"strict": true,
		"target": "es2017",
		"types": ["node"],
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"declaration": true
	},
	"compileOnSave": true,
	"include": ["src"],
	"exclude": ["src/**/*.test.ts"]
}
